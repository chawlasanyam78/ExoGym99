<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" type="image/gif/png" href="~/img/Capture.JPG">
    <title>Yoga</title>
    <link href="~/Content/bootstrap.min.cs" rel="stylesheet" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Cookie">
    <link rel="stylesheet" href="~/fonts/font-awesome.min.css">
    <link rel="stylesheet" href="~/css/styles.css">
    <link rel="stylesheet" href="~/css/Pretty-Footer.css">
</head>

<body>
   
    <h1 class="head">Yoga </h1>
    <div class="container">
        <ul class="nav nav-tabs" id="b1">
            <li class="active">@Html.ActionLink("All", "Yoga", "Health")</li>
            <li>@Html.ActionLink("Heart Disease", "Heart", "Health")</li>
            <li>@Html.ActionLink("Old", "Old", "Health")</li>
            <li>@Html.ActionLink("Young", "Young", "Health")</li>
            <li>@Html.ActionLink("Maternity", "Maternity", "Health")</li>
        </ul>
        <h6 style="text-align: right;"><b>* B-Beginner, M-intermediate, E-Expert</b></h6>
        <iframe width="560" height="315" src="https://www.youtube.com/embed/AuP6PkjaNy0" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen style="height: 246px;width: 38%;margin-left: 25px;"></iframe>
        <iframe width="560" height="315" src="https://www.youtube.com/embed/QRSbxS-uG9A" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen style="height: 246px;width: 38%;margin-left: 25px;"></iframe>
         <div class="row" id="b2">
            <div class="col-md-3">
                <div class="thumbnail"><img src="~/img/child%20pose.jpg " style="height: 180px; width: 190px;">
                    <div class="caption">
                        <h3>Child pose</h3>
                        <p>B:1 Min.<br>M:3 Mins.<br>E:6 Mins. </p>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="thumbnail"><img src="~/img/standing%20mountain.jpg" style="height: 180px; width: 190px;">
                    <div class="caption">
                        <h3>Standing mountain</h3>
                        <p>B:1 Min.<br>M:3 Mins.<br>E:6 Mins. </p>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="thumbnail"><img src="~/img/supported%20squats.jpg" style="height: 180px; width: 190px;">
                    <div class="caption">
                        <h3>Supported squat </h3>
                        <p>B:1 Min.<br>M:3 Mins.<br>E:6 Mins. </p>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="thumbnail"><img src="~/img/supported%20triangle.jpg" style="height: 180px; width: 190px;">
                    <div class="caption">
                        <h3>Supported triangle</h3>
                        <p>B:1 Min.<br>M:3 Mins.<br>E:6 Mins. </p>
                    </div>
                </div>
            </div>
        </div>
        <div class="row" id="b2">
            <div class="col-md-3">
                <div class="thumbnail"><img src="~/img/belly%20breathing.jpg" style="height: 180px; width: 190px;">
                    <div class="caption">
                        <h3>Belly breathing</h3>
                        <p>B:1 Min.<br>M:3 Mins.<br>E:6 Mins. </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
    <script src="~/js/jquery.min.js"></script>
    <script src="~/bootstrap/js/bootstrap.min.js"></script>
</body>

</html>
