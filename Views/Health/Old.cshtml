<!DOCTYPE html>
<html>



<body>
   
    <h1 class="head">Yoga </h1>
    <div class="container">
        <ul class="nav nav-tabs" id="b1">
            <li>@Html.ActionLink("All", "Yoga", "Health")</li>
            <li>@Html.ActionLink("Heart Disease", "Heart", "Health")</li>
            <li class="active">@Html.ActionLink("Old", "Old", "Health")</li>
            <li>@Html.ActionLink("Young", "Young", "Health")</li>
            <li>@Html.ActionLink("Maternity", "Maternity", "Health")</li>
        </ul>
        <h6 style="text-align: right;"><b>* B-Beginner, M-intermediate, E-Expert</b></h6>
        
        <div class="row" id="b2">
            <div class="col-md-3">
                <div class="thumbnail"><img src="~/img/lunge-pose.jpg">
                    <div class="caption">
                        <h3>Lunge Pose</h3>
                        <p>B:5 Min.<br>M:10 Mins.<br>E:15 Mins. </p>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="thumbnail"><img src="~/img/plank.jpg">
                    <div class="caption">
                        <h3>Plank </h3>
                        <p>B:1 Min.<br>M:3 Mins.<br>E:5 Mins. </p>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="thumbnail"><img src="~/img/warrior-1.jpg">
                    <div class="caption">
                        <h3>Warrior</h3>
                        <p>B:5 Min.<br>M:10 Mins.<br>E:15 Mins. </p>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="thumbnail"><img src="~/img/camel-pose.jpg">
                    <div class="caption">
                        <h3>Camel pose </h3>
                        <p>B:1 Min.<br>M:4 Mins.<br>E:10 Mins. </p>
                    </div>
                </div>
            </div>
        </div>
        <div class="row" id="b2">
            <div class="col-md-3">
                <div class="thumbnail"><img src="~/img/warrior%202.jpg">
                    <div class="caption">
                        <h3>Warrior II</h3>
                        <p>B:5 Min.<br>M:10 Mins.<br>E:15 Mins. </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
    <script src="~/js/jquery.min.js"></script>
    <script src="~/bootstrap/js/bootstrap.min.js"></script>
</body>

</html>
